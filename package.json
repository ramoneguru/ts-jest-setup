{
	"name": "signup",
	"version": "1.0.0",
	"description": "Twilio Signup implementation in React",
	"repository": "https://code.hq.twilio.com/react/signup.git",
	"config": {
		"name": "signup"
	},
	"scripts": {
		"clean": "rm -rf dist/*",
		"dev-server": "node ./node_modules/web-toolbox/scripts/dev-server.js signup",
		"build": "rm -fr dist/*; node ./node_modules/web-toolbox/scripts/build.js",
		"postversion": "git push --tags",
		"lint": "eslint src/ --ext .jsx --ext .js",
		"lint:fix": "eslint src/ --ext .jsx --ext .js --fix",
		"release": "node ./node_modules/web-toolbox/scripts/release.js --no-lint -vv --dist $1",
		"local:copy": "mkdir -p ~/Projects/php-cor/www/console/components/signup && cp -r ~/Projects/signup/dist/ ~/Projects/php-core/www/console/components/signup/",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage"
	},
	"keywords": [
		"signup",
		"react"
	],
	"author": "Twilio Inc.",
	"license": "ISC",
	"dependencies": {
		"babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
		"prop-types": "^15.5.8",
		"react": "15.5.0",
		"react-dom": "^15.5.4",
		"typescript": "^2.2.2"
	},
	"devDependencies": {
		"@types/enzyme": "^2.8.4",
		"@types/jest": "^19.2.2",
		"@types/node": "^8.0.17",
		"@types/react": "^15.0.21",
		"@types/react-dom": "^0.14.23",
		"@types/react-router": "^4.0.4",
		"assets-webpack-plugin": "^3.5.0",
		"babel-core": "^6.24.1",
		"babel-eslint": "^7.2.1",
		"babel-jest": "^19.0.0",
		"babel-loader": "^6.4.1",
		"babel-polyfill": "^6.23.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"console-js": "git+ssh://git@code.hq.twilio.com:react/console-js.git",
		"css-loader": "^0.28.0",
		"enzyme": "^2.8.1",
		"eslint": "^3.19.0",
		"eslint-plugin-react": "^6.10.3",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^19.0.2",
		"jest-cli": "^19.0.2",
		"jsx-loader": "^0.13.2",
		"less": "^2.7.2",
		"less-loader": "^4.0.3",
		"mocha": "^3.5.0",
		"react-hot-loader": "^1.3.1",
		"react-test-renderer": "^15.5.4",
		"regenerator-runtime": "^0.10.3",
		"source-map-loader": "^0.2.1",
		"style-loader": "^0.16.1",
		"ts-jest": "^19.0.9",
		"ts-loader": "^2.0.3",
		"tslint": "^5.1.0",
		"tslint-eslint-rules": "^4.0.0",
		"webpack": "^2.3.3",
		"webpack-dev-server": "^2.4.2",
		"webpack-sources": "0.2.3"
	},
	"jest": {
		"transform": {
			"^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
			".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js|jsx)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"json"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
			".less$": "<rootDir/test/helpers/lessStub.js"
		},
		"transformIgnorePatterns": [
			"/node_modules/(?!ui-components).+\\.js$"
		]
	}
}
